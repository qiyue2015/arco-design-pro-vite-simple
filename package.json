{
  "name": "admin9-pro",
  "description": "Admin9 Pro - 企业中后台管理系统，基于 Vue 和 Arco Design Pro",
  "version": "1.0.0",
  "private": true,
  "author": "Admin9 Team",
  "license": "MIT",
  "scripts": {
    "dev": "vite --config ./config/vite.config.dev.ts",
    "build": "vue-tsc --noEmit && vite build --config ./config/vite.config.prod.ts && qshell qupload qupload.conf",
    "report": "cross-env REPORT=true npm run build",
    "preview": "npm run build && vite preview --host",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "lint-staged": "npx lint-staged",
    "lint:fix": "eslint --fix \"./src/**/*.{js,ts,vue}\"",
    "prepare": "husky install",
    "new": "plop",
    "format": "prettier --write ./src && npm run lint:fix"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.vue": [
      "stylelint --fix",
      "prettier --write",
      "eslint --fix"
    ],
    "*.{less,css}": [
      "stylelint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@arco-design/web-vue": "^2.57.0",
    "@tiptap/core": "^3.0.9",
    "@vueuse/core": "^9.13.0",
    "axios": "^0.24.0",
    "dayjs": "^1.11.13",
    "echarts": "^5.6.0",
    "lodash": "^4.17.21",
    "mitt": "^3.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.3.1",
    "pinia-plugin-persistedstate": "^4.4.1",
    "query-string": "^8.2.0",
    "quill": "^2.0.3",
    "sortablejs": "^1.15.6",
    "vue": "^3.5.18",
    "vue-i18n": "^9.14.5",
    "vue-jsonp": "^2.1.0",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@arco-plugins/vite-vue": "^1.4.5",
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@tiptap/extension-color": "^2.26.1",
    "@tiptap/extension-highlight": "^2.26.1",
    "@tiptap/extension-image": "^2.26.1",
    "@tiptap/extension-placeholder": "^2.26.1",
    "@tiptap/extension-text-align": "^2.26.1",
    "@tiptap/extension-text-style": "^2.26.1",
    "@tiptap/extension-underline": "^2.26.1",
    "@tiptap/pm": "^2.26.1",
    "@tiptap/starter-kit": "^2.26.1",
    "@tiptap/vue-3": "^2.26.1",
    "@types/lodash": "^4.17.20",
    "@types/mockjs": "^1.0.10",
    "@types/nprogress": "^0.2.3",
    "@types/sortablejs": "^1.15.8",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitejs/plugin-vue-jsx": "^2.1.1",
    "@vue/babel-plugin-jsx": "^1.4.0",
    "autoprefixer": "^10.4.21",
    "consola": "^2.15.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.10.2",
    "eslint-import-resolver-typescript": "^3.10.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^4.2.5",
    "eslint-plugin-vue": "^9.33.0",
    "husky": "^8.0.3",
    "less": "^4.4.0",
    "lint-staged": "^13.3.0",
    "mockjs": "^1.1.0",
    "plop": "^4.0.1",
    "pluralize": "^8.0.0",
    "postcss": "^8.5.6",
    "postcss-html": "^1.8.0",
    "postcss-less": "^6.0.0",
    "prettier": "^2.8.8",
    "rollup": "^2.79.2",
    "rollup-plugin-visualizer": "^5.14.0",
    "stylelint": "^16.23.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-order": "^6.0.4",
    "tailwindcss": "3",
    "typescript": "^4.9.5",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^3.2.11",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-svg-loader": "^3.6.0",
    "vue-tsc": "^1.8.27"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "resolutions": {
    "bin-wrapper": "npm:bin-wrapper-china",
    "gifsicle": "5.2.0",
    "rollup": "^2.79.2"
  }
}
